{
  "name": "teo-ilie",
  "version": "2.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --configuration production && cp -r docs/browser/* docs/ && rm -rf docs/browser && rm -f docs/3rdpartylicenses.txt && rm -rf docs/prerendered-routes.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "ng lint",
    "format": "prettier --write \"src/**/*.{ts,html,scss,json}\"",
    "comp": "node convert-to-webp.js",
    "serve": "ng serve --no-hmr"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "~19.2.11",
    "@angular/common": "^19.2.8",
    "@angular/compiler": "^19.2.8",
    "@angular/core": "^19.2.8",
    "@angular/forms": "^19.2.8",
    "@angular/material": "~19.2.11",
    "@angular/platform-browser": "^19.2.8",
    "@angular/platform-browser-dynamic": "^19.2.8",
    "@angular/router": "^19.2.8",
    "angular-eslint": "^19.3.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "rxjs": "~7.8.2",
    "tslib": "^2.8.1",
    "typescript-eslint": "^8.31.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.9",
    "@angular-eslint/builder": "19.3.0",
